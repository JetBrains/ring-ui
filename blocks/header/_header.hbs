<div class="ring-header{{#type}} ring-header_{{this}}{{/type}}">
  <div class="ring-header__i">
    <div class="ring-header__services">
      {{#services}}
        {{#if active}}
          <span class="ring-header__item ring-header__item_active{{> global__class}}"{{> global__attributes}}>{{label}}</span>
        {{else}}
          <a class="ring-link ring-header__item ring-header__item_link{{> global__class}}" href="{{url}}"{{> global__attributes}}>{{label}}</a>
        {{/if}}
      {{/services}}
      {{#if items}}
        <span class="ring-header__more{{> dropdown-menu__component}}">
          <span class="ring-header__item">more</span>
          <span class="ring-font-icon ring-font-icon_caret-down"></span>
        </span>
      {{/if}}
    </div>
    {{#personal}}
      <span class="ring-header__personal{{> dropdown-menu__component}}">
        <span class="ring-header__item">{{username}}</span>
        <span class="ring-font-icon ring-font-icon_caret-down"></span>
      </span>
    {{else}}
      {{#authLinks/login}}
        <span class="ring-header__personal">
          {{#if url}}
            <a class="ring-link ring-header__item ring-header__item_link{{> global__class}}" {{> global__attributes}} href="{{url}}">{{label}}</a>
          {{else}}
            <span class="ring-link ring-header__item ring-header__item_link{{> global__class}}" {{> global__attributes}}>{{label}}</span>
          {{/if}}
        </span>
      {{/authLinks/login}}
    {{/personal}}
  </div>
</div>