{
  "name": "@jetbrains/ring-ui",
  "version": "0.2.15",
  "description": "JetBrains UI library",
  "author": "JetBrains",
  "license": "Apache-2.0",
  "files": [
    "components/**/*.{js,css,scss,gif,html,svg}",
    "postcss.config.js",
    "webpack.config.js",
    "icon-runtime-generator.js",
    "jslint-xml.js"
  ],
  "main": "components/index.js",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix --ignore-path .eslintignore",
      "git add"
    ]
  },
  "config": {
    "components": "./components/"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "bootstrap": "lerna bootstrap",
    "precommit": "lint-staged",
    "component:ng": "yo @jetbrains/ring-ui:ng --path $npm_package_config_components",
    "component:react": "yo @jetbrains/ring-ui:react --path $npm_package_config_components",
    "component:es6": "yo @jetbrains/ring-ui:es6 --path $npm_package_config_components",
    "test": "karma start",
    "test-generator-e2e": "npm --prefix packages/generatorgenerator test",
    "gemini-test": "npm --prefix packages/gemini run gemini-test",
    "gemini-test-ci": "npm --prefix packages/gemini run gemini-test-ci",
    "gemini-gather": "npm --prefix packages/gemini run gemini-gather",
    "build": "npm --prefix packages/docs run build",
    "build-prod": "npm --prefix packages/docs run build-prod",
    "start": "npm --prefix packages/docs run start",
    "lint": "eslint . ; npm run stylelint",
    "prelint-ci": "echo \"##teamcity[importData type='jslint' path='eslint-report.xml']\"",
    "lint-ci": "eslint --format jslint-xml . > eslint-report.xml; npm run stylelint-ci",
    "stylelint-ci": "stylelint --ignore-path .eslintignore --custom-formatter './jslint-xml.js' '**/*.css' | xmlappend eslint-report.xml",
    "stylelint": "stylelint --ignore-path .eslintignore '**/*.css'",
    "flow": "flow",
    "test-crossbrowser": "karma start karma-crossbrowser.conf.js",
    "pretest-ci": "npm run lint-ci",
    "test-ci": "xvfb-run karma start karma-ci.conf.js",
    "test-watch": "karma start karma-watch.conf.js",
    "prerelease-ci": "git pull",
    "release-ci": "lerna publish --yes"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/JetBrains/ring-ui.git"
  },
  "readmeFilename": "README.md",
  "babel": {
    "presets": "@jetbrains/jetbrains",
    "plugins": [
      [
        "transform-runtime",
        {
          "moduleName": "@jetbrains/babel-runtime"
        }
      ],
      [
        "@jetbrains/babel-plugin-angular-annotate",
        [
          "angular",
          "ngRoute"
        ]
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "istanbul",
            {
              "exclude": [
                "**/*.test.js"
              ]
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "@jetbrains/eslint-config": "^2.0.0",
    "@jetbrains/generator-ring-ui": "^0.2.15",
    "@jetbrains/stylelint-config": "^1.3.1",
    "angular": "1.6.5",
    "angular-mocks": "1.6.5",
    "angular-route": "1.6.5",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-istanbul": "3.1.2",
    "babel-polyfill": "6.13.0",
    "chai": "4.1.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "1.5.0",
    "chai-enzyme": "0.8.0",
    "change-case": "^3.0.1",
    "cheerio": "^0.22.0",
    "css-module-flow": "^1.0.0",
    "deep-assign": "2.0.0",
    "docpack": "1.0.2",
    "docpack-examples-compiler": "0.0.3",
    "docpack-jsdoc-extractor": "0.0.6",
    "docpack-markdown-extractor": "0.0.2",
    "docpack-page-generator": "0.0.6",
    "electron": "1.6.11",
    "enzyme": "2.9.1",
    "eslint": "^4.2.0",
    "eslint-plugin-flowtype-errors": "^3.3.0",
    "exports-loader": "0.6.4",
    "flow-bin": "0.44.2",
    "front-matter": "2.1.2",
    "gen-idea-libs": "1.0.2",
    "glob": "7.1.2",
    "husky": "0.14.3",
    "iframe-resizer": "3.5.14",
    "json-loader": "0.5.7",
    "karma": "1.7.0",
    "karma-coverage": "1.1.1",
    "karma-coverage-html-index-reporter": "1.0.2",
    "karma-electron-launcher": "0.2.0",
    "karma-mocha": "1.3.0",
    "karma-osx-reporter": "0.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "1.0.0",
    "karma-webdriver-launcher": "1.0.5",
    "karma-webpack": "2.0.4",
    "lerna": "^2.1.2",
    "lint-staged": "4.0.2",
    "mocha": "3.4.2",
    "react-github-corner": "^1.0.0",
    "react-test-renderer": "15.6.1",
    "sinon": "2.3.8",
    "sinon-chai": "2.12.0",
    "storage-mock": "1.0.3",
    "stylelint": "8.0.0",
    "wallaby-webpack": "0.0.38",
    "webpack": "2.7.0",
    "webpack-dev-server": "2.6.1",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "xmlappend": "1.0.4",
    "yo": "2.0.0"
  },
  "dependencies": {
    "@jetbrains/angular-elastic": "2.5.1",
    "@jetbrains/babel-plugin-angular-annotate": "2.0.3",
    "@jetbrains/babel-preset-jetbrains": "1.0.1",
    "@jetbrains/babel-runtime": "6.23.1",
    "@jetbrains/icons": "1.4.0",
    "@jetbrains/logos": "0.1.0",
    "@jetbrains/react-portal": "3.1.0",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "classnames": "2.2.5",
    "combokeys": "2.4.6",
    "compile-code-loader": "0.2.0",
    "css-loader": "0.28.4",
    "deep-equal": "1.0.1",
    "dom4": "1.8.5",
    "element-resize-detector": "1.1.12",
    "es6-error": "^4.0.2",
    "extricate-loader": "^1.0.0",
    "file-loader": "0.11.2",
    "focus-trap": "2.3.0",
    "focus-trap-react": "3.0.3",
    "highlight.js": "9.12.0",
    "html-loader": "0.4.5",
    "imports-loader": "0.7.1",
    "interpolate-loader": "^1.0.0",
    "just-debounce-it": "1.0.1",
    "loader-utils": "1.1.0",
    "moment": "2.18.1",
    "node-sass": "4.5.3",
    "pascal-case": "2.0.1",
    "postcss": "6.0.8",
    "postcss-cssnext": "2.11.0",
    "postcss-loader": "1.3.3",
    "postcss-modules-values-replace": "1.0.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-markdown": "2.5.0",
    "react-sortable-hoc": "0.6.5",
    "react-virtualized": "9.9.0",
    "react-waypoint": "7.0.4",
    "sass-loader": "6.0.6",
    "scrollbar-width": "3.1.1",
    "simply-uuid": "1.0.1",
    "sniffr": "1.1.4",
    "style-loader": "0.18.2",
    "svg-sprite-loader": "3.2.5",
    "url-loader": "0.5.9"
  }
}
