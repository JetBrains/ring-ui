{
  "name": "ring-ui",
  "version": "2.1.0",
  "description": "JetBrains UI library",
  "config": {
    "dist": "./dist",
    "src": "./src",
    "eslint-ext": ".js,.jsx",
    "eslint-report": "eslint-report.xml"
  },
  "files": [
    "/ring.js",
    "/ring2.js",
    "components/**/*.{jsx,js,scss,png,gif,svg,ttf,woff,eof,html}",
    "webpack.config.js"
  ],
  "main": "ring.js",
  "scripts": {
    "prepublish": "node tools/setup-hooks.js",
    "gen-doc": "node tools/generate-documentation.js",
    "test": "node node_modules/karma/bin/karma start",
    "build": "gulp build",
    "clean": "gulp clean",
    "start": "gulp",
    "lint": "eslint --ext $npm_package_config_eslint_ext .",
    "lint-hook": "eslint --ext $npm_package_config_eslint_ext $ESLINT_HOOK_FILES",
    "lint-ci": "eslint --ext $npm_package_config_eslint_ext --format jslint-xml . > $npm_package_config_eslint_report",
    "postlint-ci": "node tools/import-reports",
    "test-ci": "node node_modules/karma/bin/karma start karma-ci.conf.js",
    "prebuild-ci": "npm run lint-ci && npm run test-ci",
    "build-ci": "gulp build && npm run gen-doc",
    "prerelease-ci": "npm run build-ci",
    "release-ci": "npm version $ring_ui_version && npm publish",
    "postrelease-ci": "npm pack"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/JetBrains/ring-ui.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "angular": "1.3.8",
    "angular-mocks": "1.3.8",
    "angular-route": "1.3.8",
    "dgeni": "0.4.1",
    "dgeni-packages": "0.10.8",
    "eslint": "0.15.0",
    "eslint-plugin-angular": "0.0.3",
    "eslint-plugin-filenames": "0.1.0",
    "eslint-plugin-google-camelcase": "0.0.1",
    "eslint-plugin-mocha": "0.2.2",
    "eslint-plugin-one-variable-per-var": "0.0.3",
    "eslint-plugin-react": "1.2.2",
    "gulp": "3.8.10",
    "gulp-concat": "2.4.3",
    "gulp-nodemon": "1.0.5",
    "gulp-rimraf": "0.1.1",
    "gulp-util": "3.0.2",
    "handlebars": "2.0.0",
    "jquery": "2.1.3",
    "karma": "0.12.31",
    "karma-chai-plugins": "0.2.4",
    "karma-chrome-launcher": "0.1.7",
    "karma-firefox-launcher": "0.1.4",
    "karma-mocha": "0.1.10",
    "karma-phantomjs-launcher": "0.1.4",
    "karma-teamcity-reporter": "0.1.2",
    "karma-webpack": "1.5.0",
    "mocha": "2.1.0",
    "react-tools": "0.11.2",
    "through": "2.3.6",
    "webpack-dev-server": "1.7.0",
    "yargs": "1.3.3"
  },
  "dependencies": {
    "autoprefixer-loader": "1.0.0",
    "codemirror": "4.11.0",
    "css-loader": "0.9.1",
    "es5-shim": "4.0.5",
    "exports-loader": "0.6.2",
    "file-loader": "0.8.1",
    "html-loader": "0.2.3",
    "imports-loader": "0.6.3",
    "jsx-loader": "0.11.2",
    "memory-fs": "0.1.1",
    "mousetrap": "1.4.6",
    "mout": "0.11.0",
    "node-xml-lite": "0.0.3",
    "q-fs": "0.1.36",
    "react": "0.11.2",
    "sass-loader": "0.3.1",
    "style-loader": "0.8.2",
    "url-loader": "0.5.5",
    "val-loader": "0.5.0",
    "webpack": "1.4.15",
    "when": "3.6.4",
    "xml2js": "0.4.4"
  }
}
