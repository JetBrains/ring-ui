{
  "name": "ring-ui",
  "version": "2.3.0",
  "description": "JetBrains UI library",
  "config": {
    "eslint-report": "eslint-report.xml",
    "port": 9999,
    "version": "minor"
  },
  "files": [
    "ring-upsource.js",
    "components/**/*.{js,scss,gif,html}",
    "webpack.config.js"
  ],
  "main": "webpack.config.js",
  "scripts": {
    "eslint-fix": "eslint --format 'node_modules/eslint-friendly-formatter' .",
    "test": "karma start",
    "gemini-test": "node tools/gemini-runner.js --files components/**/*.gemini.js",
    "gemini-test-ci": "node tools/gemini-runner-ci.js",
    "gemini-gather": "node tools/gemini-runner.js --gather",
    "build-upsource": "webpack tools/ring-upsource.js ring-upsource.js --output-library=ring --bail --optimize-dedupe --optimize-minimize --optimize-occurence-order --define process.env.NODE_ENV=\"'production'\"",
    "build": "node tools/metalsmith.js",
    "start": "node tools/metalsmith.js --server",
    "lint": "eslint .",
    "lint-hook": "eslint $ESLINT_HOOK_FILES",
    "prelint-ci": "node tools/message-import-reports",
    "lint-ci": "eslint --format jslint-xml . > $npm_package_config_eslint_report",
    "test-ci": "karma start karma-ci.conf.js",
    "test-watch": "karma start karma-watch.conf.js",
    "prebuild-ci": "npm run lint-ci && npm run test-ci",
    "build-ci": "npm run build-upsource && npm run build",
    "postbuild-ci": "npm run gemini-test-ci",
    "prerelease-ci-backup": "npm run build-ci && npm version $npm_package_config_version && npm pack -ddd && (tar -tzvf ring-ui-*.tgz | grep webpack.config.js || node tools/message-build-fail)",
    "pack": "tools/pack.sh",
    "prerelease-ci": "npm run build-ci && npm run pack",
    "release-ci": "npm publish ring-ui-*.tgz"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/JetBrains/ring-ui.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "angular": "1.4.7",
    "angular-mocks": "1.4.7",
    "angular-route": "1.4.7",
    "anybar-webpack": "1.0.8",
    "babel": "5.8.21",
    "babel-eslint": "4.1.1",
    "chalk": "0.4.0",
    "debug-decorator": "0.0.3",
    "dox": "0.7.1",
    "eslint": "1.8.0",
    "eslint-config-jetbrains": "1.0.11",
    "finalhandler": "0.3.5",
    "gemini": "0.11.3",
    "gemini-teamcity": "1.0.1",
    "github-markdown-css": "2.0.7",
    "handlebars": "2.0.0",
    "highlight.js": "8.5.0",
    "isparta-instrumenter-loader": "0.2.1",
    "jquery": "2.1.3",
    "js-beautify": "1.5.5",
    "jsdoctypeparser": "1.1.4",
    "karma": "0.13.10",
    "karma-chai-plugins": "0.5.1",
    "karma-chrome-launcher": "0.2.0",
    "karma-coverage": "0.5.3",
    "karma-firefox-launcher": "0.1.6",
    "karma-mocha": "0.2.0",
    "karma-nodewebkit-launcher": "0.0.12",
    "karma-nyan-reporter": "0.2.2",
    "karma-osx-reporter": "0.2.0",
    "karma-phantomjs-shim": "1.1.1",
    "karma-teamcity-reporter": "0.2.1",
    "karma-webdriver-launcher": "1.0.4",
    "karma-webpack": "1.7.0",
    "metalsmith": "1.5.0",
    "metalsmith-collections": "0.7.0",
    "metalsmith-filepath": "1.0.1",
    "metalsmith-ignore": "0.1.2",
    "metalsmith-markdown": "0.2.1",
    "metalsmith-metallic": "0.3.1",
    "metalsmith-templates": "0.7.0",
    "metalsmith-text-replace": "1.0.1",
    "metalsmith-watch": "0.2.0",
    "mocha": "2.3.3",
    "serve-static": "1.9.2",
    "slug": "0.8.0",
    "wallaby-webpack": "0.0.7",
    "webpack-config-merger": "0.0.3",
    "webpack-dev-server": "1.11.0"
  },
  "dependencies": {
    "angular-elastic": "2.5.1-jb",
    "autoprefixer-loader": "1.0.0",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.24",
    "classnames": "2.1.3",
    "core-js": "1.1.4",
    "css-loader": "0.9.1",
    "debounce": "1.0.0",
    "deep-assign": "2.0.0",
    "dom4": "1.4.6",
    "exports-loader": "0.6.2",
    "file-loader": "0.8.1",
    "html-loader": "0.2.3",
    "imports-loader": "0.6.3",
    "jetbrains-icons": "1.0.4",
    "jetbrains-logos": "0.0.14",
    "mousetrap": "1.4.6",
    "mout": "0.11.0",
    "ng-annotate-loader": "0.0.6",
    "node-sass": "3.3.3",
    "node-xml-lite": "0.0.3",
    "react": "0.14.0",
    "react-addons-css-transition-group": "0.14.0",
    "react-addons-test-utils": "0.14.0",
    "react-dom": "0.14.0",
    "sass-loader": "2.0.1",
    "simply-uuid": "1.0.1",
    "sniffr": "0.1.1",
    "style-loader": "0.8.2",
    "svg-sprite-loader": "0.0.13",
    "svgo": "0.5.6",
    "svgo-loader": "1.1.0",
    "url-loader": "0.5.5",
    "webpack": "1.12.2",
    "whatwg-fetch": "0.7.0",
    "xml2js": "0.4.4"
  }
}
