    "prop-types": "15.6.2",
    "react-markdown": "4.0.3",
    "react-sortable-hoc": "0.6.8",
    "react-virtualized": "9.20.1",
    "react-waypoint": "8.0.3",
    "remark-breaks": "1.0.1",
    "sass": "1.14.3",
    "sass-loader": "7.1.0",
    "scrollbar-width": "3.1.1",
    "simply-uuid": "1.0.1",
    "sniffr": "1.1.4",
    "style-loader": "0.23.1",
    "svg-sprite-loader": "4.1.2",
    "url-loader": "1.1.2",
    "url-search-params": "^0.10.2",
    "util-deprecate": "^1.0.2",
    "whatwg-fetch": "3.0.0"
  },
  "engines": {
    "node": ">=7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
