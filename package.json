{
  "name": "ring-ui",
  "version": "2.5.0",
  "description": "JetBrains UI library",
  "config": {
    "port": 9999,
    "hub": "default",
    "hubs": {
      "local": "http://localhost:8080/jetpass",
      "jar": "http://localhost:8080",
      "default": "***REMOVED***",
      "production": "https://hub.jetbrains.com"
    },
    "host": "localhost",
    "components": "./components/"
  },
  "files": [
    "components/**/*.{js,css,scss,gif,html,svg}",
    "webpack.config.js"
  ],
  "main": "webpack.config.js",
  "scripts": {
    "component:ng": "yo ring-ui:ng --path $npm_package_config_components",
    "component:react": "yo ring-ui:react --path $npm_package_config_components",
    "component:es6": "yo ring-ui:es6 --path $npm_package_config_components",
    "test": "karma start",
    "gemini-test": "gemini test components/**/*.gemini.js --reporter html --reporter flat",
    "gemini-test-ci": "forever start .serve-static.json && npm run gemini-test -- --teamcity; forever stop serve-static",
    "gemini-gather": "gemini update",
    "build": "webpack --config webpack-docs.config.js",
    "start": "node --max_old_space_size=2000 ./node_modules/.bin/webpack-dev-server --config webpack-docs.config.js --port 9999 --server --host 0.0.0.0",
    "lint": "eslint --quiet .",
    "prelint-ci": "echo \"##teamcity[importData type='jslint' path='eslint-report.xml']\"",
    "lint-ci": "eslint --format jslint-xml . > eslint-report.xml",
    "nibble": "eslint-nibble .",
    "test-ci": "karma start karma-ci.conf.js",
    "test-watch": "karma start karma-watch.conf.js",
    "prebuild-ci": "npm run lint-ci && npm run test-ci",
    "build-ci": "npm run build && npm run gemini-test-ci",
    "prerelease-ci": "npm run build-ci && npm pack",
    "release-ci": "npm publish ring-ui-*.tgz",
    "version": "update-version --patch"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/JetBrains/ring-ui.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "readmeFilename": "README.md",
  "babel": {
    "presets": "jetbrains",
    "plugins": "transform-runtime"
  },
  "devDependencies": {
    "angular": "1.5.8",
    "angular-mocks": "1.5.8",
    "angular-route": "1.5.8",
    "anybar-webpack": "1.2.0",
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-polyfill": "6.13.0",
    "deep-assign": "2.0.0",
    "electron-prebuilt": "1.3.5",
    "eslint": "3.5.0",
    "eslint-config-jetbrains": "2.0.18",
    "eslint-nibble": "3.0.0",
    "exports-loader": "0.6.3",
    "extract-loader": "0.0.2",
    "forever": "0.15.2",
    "gemini": "4.9.3",
    "gemini-teamcity": "2.1.3",
    "generator-ring-ui": "1.0.24",
    "github-markdown-css": "2.4.0",
    "glob": "7.0.6",
    "highlight.js": "9.6.0",
    "http-server": "0.9.0",
    "isparta-instrumenter-loader": "1.0.1",
    "js-beautify": "1.6.4",
    "karma": "1.3.0",
    "karma-chai-plugins": "0.7.0",
    "karma-coverage": "1.1.1",
    "karma-coverage-html-index-reporter": "1.0.2",
    "karma-electron-launcher": "0.1.0",
    "karma-mocha": "1.1.1",
    "karma-osx-reporter": "0.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "1.0.0",
    "karma-webdriver-launcher": "1.0.4",
    "karma-webpack": "1.8.0",
    "mocha": "3.0.2",
    "update-version": "1.0.1",
    "wallaby-webpack": "0.0.23",
    "webpack": "1.13.2",
    "webpack-config-merger": "0.0.5",
    "webpack-dev-server": "1.15.1",
    "webpack-docs-plugin": "1.0.12-alpha",
    "yo": "1.8.5"
  },
  "dependencies": {
    "angular-elastic": "2.5.1-jb",
    "autoprefixer": "6.4.1",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-jetbrains": "1.0.2",
    "babel-runtime": "6.6.1-jb",
    "classnames": "2.2.5",
    "combokeys": "2.4.6",
    "core-js": "2.4.1",
    "css-element-queries": "0.3.2",
    "css-loader": "0.25.0",
    "dom4": "1.8.3",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "imports-loader": "0.6.5",
    "jetbrains-icons": "1.0.27",
    "jetbrains-logos": "1.1.51",
    "json-loader": "0.5.4",
    "moment": "2.15.2",
    "mout": "1.0.0",
    "ng-annotate-loader": "0.1.1",
    "node-sass": "3.9.3",
    "null-loader": "0.1.1",
    "postcss": "5.2.0",
    "postcss-cssnext": "2.8.0",
    "postcss-loader": "1.1.0",
    "postcss-modules-values-replace": "1.0.0",
    "react": "15.3.1",
    "react-addons-css-transition-group": "15.3.1",
    "react-addons-test-utils": "15.3.1",
    "react-dom": "15.3.1",
    "react-waypoint": "4.0.2",
    "sass-loader": "4.0.2",
    "scrollbar-width": "3.1.1",
    "simply-uuid": "1.0.1",
    "sniffr": "1.1.4",
    "style-loader": "0.13.1",
    "svg-sprite-loader": "0.0.27",
    "url-loader": "0.5.7",
    "whatwg-fetch": "1.0.0"
  }
}
