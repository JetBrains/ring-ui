{
  "name": "@jetbrains/ring-ui",
  "version": "2.0.4",
  "description": "JetBrains UI library",
  "author": "JetBrains",
  "license": "Apache-2.0",
  "files": [
    ".babelrc",
    "components",
    "postcss.config.js",
    "webpack.config.js",
    "extract-css-vars.js",
    "jslint-xml.js"
  ],
  "main": "components/index.js",
  "lint-staged": {
    "*.js": [
      "npm run lint:js --fix --",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "config": {
    "components": "./components/"
  },
  "scripts": {
    "prebootstrap": "yarn install",
    "bootstrap": "lerna bootstrap",
    "component:ng": "yo @jetbrains/ring-ui:ng --path $npm_package_config_components",
    "component:react": "yo @jetbrains/ring-ui:react --path $npm_package_config_components",
    "component:es6": "yo @jetbrains/ring-ui:es6 --path $npm_package_config_components",
    "test": "karma start",
    "test-generator-e2e": "yarn --cwd packages/generator test",
    "gemini-gui": "yarn --cwd packages/gemini run gemini-gui",
    "gemini-test": "yarn --cwd packages/gemini run gemini-test",
    "gemini-test-single": "yarn --cwd packages/gemini run gemini-test-single",
    "gemini-test-ci": "yarn --cwd packages/gemini run gemini-test-ci",
    "gemini-gather": "yarn --cwd packages/gemini run gemini-gather",
    "build": "yarn --cwd packages/docs run build",
    "build-prod": "yarn --cwd packages/docs run build-prod",
    "start": "yarn --cwd packages/docs run start",
    "lint:js": "eslint --report-unused-disable-directives",
    "lint": "npm run lint:js . && npm run stylelint",
    "prelint-ci": "echo \"##teamcity[importData type='jslint' path='eslint-report.xml']\"",
    "lint-ci": "eslint --report-unused-disable-directives --format jslint-xml . > eslint-report.xml && npm run stylelint-ci",
    "stylelint-ci": "stylelint --ignore-path .eslintignore --custom-formatter './jslint-xml.js' '**/*.css' | xmlappend eslint-report.xml",
    "stylelint": "stylelint --ignore-path .eslintignore '**/*.css'",
    "test-crossbrowser": "karma start karma-crossbrowser.conf.js",
    "pretest-ci": "npm run lint-ci",
    "test-ci": "xvfb-maybe karma start karma-ci.conf.js",
    "test-watch": "karma start karma-watch.conf.js",
    "prerelease-ci": "git pull",
    "release-ci": "lerna publish --yes"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/JetBrains/ring-ui.git"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@jetbrains/eslint-config": "^4.0.5",
    "@jetbrains/generator-ring-ui": "^1.0.40",
    "@jetbrains/stylelint-config": "^1.3.4",
    "angular": "^1.7.8",
    "angular-mocks": "^1.7.8",
    "angular-route": "^1.7.8",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.8.1",
    "chai-enzyme": "1.0.0-beta.1",
    "change-case": "^3.1.0",
    "cheerio": "^0.22.0",
    "core-js": "^2.6.5",
    "css-module-flow": "^1.0.0",
    "deep-assign": "^3.0.0",
    "docpack": "^1.0.6",
    "docpack-examples-compiler": "^0.0.6",
    "docpack-jsdoc-extractor": "^0.0.8",
    "docpack-markdown-extractor": "^0.0.4",
    "docpack-page-generator": "^0.0.11",
    "electron": "^4.1.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "front-matter": "^3.0.2",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "iframe-resizer": "^4.1.1",
    "json-loader": "^0.5.7",
    "karma": "^4.1.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-html-index-reporter": "^1.0.2",
    "karma-electron-launcher": "^0.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^2.0.0",
    "karma-webdriver-launcher": "^1.0.5",
    "karma-webpack": "^3.0.0",
    "lerna": "^3.13.4",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "octicons": "^8.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "recompose": "^0.30.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "storage-mock": "^1.0.3",
    "stylelint": "^10.0.1",
    "wallaby-webpack": "^3.9.15",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.11.5",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-toolkit": "^1.0.0",
    "xmlappend": "^1.0.4",
    "xvfb-maybe": "^0.2.1",
    "yo": "^2.0.6"
  },
  "peerDependencies": {
    "core-js": ">=2.0.0",
    "react": ">=16.4.0",
    "react-dom": ">=16.4.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@jetbrains/angular-elastic": "^2.5.1",
    "@jetbrains/babel-preset-jetbrains": "^2.1.4",
    "@jetbrains/icons": "^3.1.0",
    "@jetbrains/logos": "^1.1.4",
    "@jetbrains/postcss-require-hover": "^0.1.2",
    "angular-sanitize": "^1.7.8",
    "babel-loader": "^8.0.5",
    "classnames": "^2.2.6",
    "combokeys": "^3.0.1",
    "compile-code-loader": "^0.2.0",
    "conic-gradient": "^1.0.0",
    "css-loader": "^1.0.1",
    "deep-equal": "^1.0.1",
    "dom4": "^1.8.5",
    "element-resize-detector": "^1.2.0",
    "es6-error": "^4.1.1",
    "extricate-loader": "^3.0.0",
    "file-loader": "^2.0.0",
    "focus-trap": "^5.0.0",
    "highlight.js": "^9.15.6",
    "html-loader": "^0.5.5",
    "imports-loader": "^0.8.0",
    "interpolate-loader": "^1.0.0",
    "just-debounce-it": "^1.1.0",
    "moment": "^2.24.0",
