{
  "name": "ring-ui",
  "version": "2.1.0",
  "description": "JetBrains UI library",
  "config": {
    "eslint-ext": ".js,.jsx",
    "eslint-report": "eslint-report.xml",
    "port": 9999,
    "version": "minor"
  },
  "files": [
    "/ring-upsource.js",
    "components/**/*.{jsx,js,scss,png,gif,svg,ttf,woff,eof,html}",
    "webpack.config.js"
  ],
  "main": "ring.js",
  "scripts": {
    "prepublish": "node tools/setup-hooks.js",
    "gen-doc": "node tools/generate-documentation.js",
    "watch-doc": "node tools/watch-doc.js",
    "test": "node node_modules/karma/bin/karma start",
    "build-upsource": "webpack tools/ring-upsource.js ring-upsource.js --output-library=ring --bail --optimize-dedupe --optimize-minimize --optimize-occurence-order --define process.env.NODE_ENV=\"'production'\"",
    "build": "node tools/metalsmith.js",
    "start": "node tools/metalsmith.js --server",
    "lint": "eslint --ext $npm_package_config_eslint_ext .",
    "lint-hook": "eslint --ext $npm_package_config_eslint_ext $ESLINT_HOOK_FILES",
    "prelint-ci": "npm run lint",
    "lint-ci": "eslint --ext $npm_package_config_eslint_ext --format jslint-xml . > $npm_package_config_eslint_report",
    "postlint-ci": "node tools/import-reports",
    "test-ci": "node node_modules/karma/bin/karma start karma-ci.conf.js",
    "prebuild-ci": "npm run lint-ci && npm run test-ci",
    "build-ci": "npm run build-upsource && npm run build",
    "prerelease-ci": "npm run build-ci",
    "release-ci": "npm version $npm_package_config_version && npm publish",
    "postrelease-ci": "npm pack"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/JetBrains/ring-ui.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "angular": "1.3.15",
    "angular-mocks": "1.3.15",
    "angular-route": "1.3.15",
    "anybar-webpack": "1.0.6",
    "babel": "4.7.16",
    "dox": "0.7.1",
    "eslint": "0.17.1",
    "eslint-plugin-angular": "0.0.6",
    "eslint-plugin-filenames": "0.1.0",
    "eslint-plugin-google-camelcase": "0.0.1",
    "eslint-plugin-mocha": "0.2.2",
    "eslint-plugin-one-variable-per-var": "0.0.3",
    "eslint-plugin-react": "1.6.0",
    "github-markdown-css": "2.0.7",
    "handlebars": "2.0.0",
    "jquery": "2.1.3",
    "js-beautify": "1.5.5",
    "karma": "0.12.31",
    "karma-chai-plugins": "0.4.0",
    "karma-chrome-launcher": "0.1.7",
    "karma-firefox-launcher": "0.1.4",
    "karma-mocha": "0.1.10",
    "karma-phantomjs-launcher": "0.1.4",
    "karma-teamcity-reporter": "0.1.2",
    "karma-webdriver-launcher": "1.0.1",
    "karma-webpack": "1.5.0",
    "metalsmith": "1.5.0",
    "metalsmith-collections": "0.7.0",
    "metalsmith-filepath": "1.0.1",
    "metalsmith-ignore": "0.1.2",
    "metalsmith-markdown": "0.2.1",
    "metalsmith-metallic": "0.3.1",
    "metalsmith-static": "0.0.5",
    "metalsmith-templates": "0.7.0",
    "metalsmith-watch": "0.2.0",
    "mocha": "2.1.0",
    "react-tools": "0.11.2",
    "slug": "0.8.0",
    "wallaby-webpack": "0.0.4",
    "webpack-dev-server": "1.8.0",
    "xml-parser": "1.2.0"
  },
  "dependencies": {
    "angular-elastic": "2.3.5",
    "autoprefixer-loader": "1.0.0",
    "bowser": "0.7.2",
    "codemirror": "4.11.0",
    "css-loader": "0.9.1",
    "es5-shim": "4.0.5",
    "exports-loader": "0.6.2",
    "file-loader": "0.8.1",
    "fuzzysearch": "1.0.3",
    "html-loader": "0.2.3",
    "imports-loader": "0.6.3",
    "jsx-loader": "0.11.2",
    "mousetrap": "1.4.6",
    "mout": "0.11.0",
    "ng-annotate-loader": "0.0.2",
    "node-xml-lite": "0.0.3",
    "react": "0.11.2",
    "sass-loader": "0.4.2",
    "style-loader": "0.8.2",
    "url-loader": "0.5.5",
    "val-loader": "0.5.0",
    "webpack": "1.8.2",
    "whatwg-fetch": "0.7.0",
    "when": "3.6.4",
    "xml2js": "0.4.4"
  }
}
