{
  "name": "ring-ui",
  "version": "2.4.0",
  "description": "JetBrains UI library",
  "config": {
    "eslint-report": "eslint-report.xml",
    "port": 9999,
    "version": "minor",
    "hub": "default",
    "hubs": {
      "local": "http://localhost:8080/jetpass",
      "jar": "http://localhost:8080",
      "default": "***REMOVED***",
      "production": "https://hub.jetbrains.com"
    },
    "host": "localhost"
  },
  "files": [
    "components/**/*.{js,scss,gif,html,svg}",
    "webpack.config.js"
  ],
  "main": "webpack.config.js",
  "scripts": {
    "test": "karma start",
    "setup-hooks": "node tools/setup-hooks.js",
    "gemini-test": "babel-node tools/gemini-runner.js components/**/*.gemini.js",
    "gemini-test-ci": "babel-node tools/gemini-runner-ci.js",
    "gemini-gather": "babel-node tools/gemini-runner.js --gather",
    "build": "webpack --config webpack-docs.config.js",
    "start": "webpack-dev-server --config webpack-docs.config.js --port 9999 --server",
    "lint": "eslint --quiet .",
    "lint-hook": "eslint $ESLINT_HOOK_FILES",
    "prelint-ci": "babel-node tools/message-import-reports",
    "lint-ci": "eslint --format jslint-xml . > $npm_package_config_eslint_report",
    "nibble": "eslint-nibble .",
    "test-ci": "karma start karma-ci.conf.js",
    "test-watch": "karma start karma-watch.conf.js",
    "prebuild-ci": "npm run lint-ci && npm run test-ci",
    "build-ci": "npm run build && npm run gemini-test-ci",
    "prerelease-ci": "npm run build-ci && npm version $npm_package_config_version && npm pack",
    "release-ci": "npm publish ring-ui-*.tgz"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/JetBrains/ring-ui.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "readmeFilename": "README.md",
  "babel": {
    "presets": "jetbrains"
  },
  "devDependencies": {
    "angular": "1.5.6",
    "angular-mocks": "1.5.6",
    "angular-route": "1.5.6",
    "anybar-webpack": "1.0.8",
    "babel-cli": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-polyfill": "6.8.0",
    "chalk": "0.4.0",
    "deep-assign": "2.0.0",
    "dox": "0.7.1",
    "electron-prebuilt": "0.36.9",
    "escope": "3.2.0",
    "eslint": "2.9.0",
    "eslint-config-jetbrains": "2.0.18",
    "eslint-nibble": "2.0.0",
    "exports-loader": "0.6.2",
    "extract-loader": "0.0.2",
    "finalhandler": "0.3.5",
    "gemini": "2.1.1",
    "gemini-teamcity": "2.1.1",
    "github-markdown-css": "2.0.7",
    "handlebars": "2.0.0",
    "highlight.js": "8.5.0",
    "isparta-instrumenter-loader": "1.0.0",
    "js-beautify": "1.5.5",
    "jsdoctypeparser": "1.1.4",
    "karma": "0.13.19",
    "karma-chai-plugins": "0.6.1",
    "karma-chrome-launcher": "0.2.0",
    "karma-coverage": "0.5.3",
    "karma-coverage-html-index-reporter": "1.0.2",
    "karma-electron-launcher": "0.0.5",
    "karma-firefox-launcher": "0.1.6",
    "karma-mocha": "0.2.0",
    "karma-osx-reporter": "0.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "0.2.1",
    "karma-webdriver-launcher": "1.0.4",
    "karma-webpack": "1.7.0",
    "metalsmith": "1.5.0",
    "metalsmith-collections": "0.7.0",
    "metalsmith-filepath": "1.0.1",
    "metalsmith-ignore": "0.1.2",
    "metalsmith-markdown": "0.2.1",
    "metalsmith-metallic": "0.3.1",
    "metalsmith-templates": "0.7.0",
    "metalsmith-text-replace": "1.0.1",
    "metalsmith-watch": "1.0.1",
    "mocha": "2.3.3",
    "serve-static": "1.9.2",
    "slug": "0.8.0",
    "wallaby-webpack": "0.0.22",
    "webpack": "1.13.0",
    "webpack-config-merger": "0.0.5",
    "webpack-dev-server": "1.14.1",
    "webpack-docs-plugin": "1.0.4-alpha"
  },
  "dependencies": {
    "angular-elastic": "2.5.1-jb",
    "angular-es6": "3.0.5",
    "autoprefixer": "6.3.6",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-jetbrains": "1.0.1",
    "babel-runtime": "6.6.1-jb",
    "classnames": "2.1.3",
    "combokeys": "2.4.6",
    "core-js": "1.1.4",
    "css-element-queries": "0.3.2",
    "css-loader": "0.12.0",
    "dom4": "1.4.6",
    "file-loader": "0.8.1",
    "html-loader": "0.4.0",
    "imports-loader": "0.6.3",
    "jetbrains-icons": "1.0.21",
    "jetbrains-logos": "1.1.31",
    "mout": "0.11.0",
    "ng-annotate-loader": "0.0.6",
    "node-sass": "3.7.0",
    "postcss": "5.0.19",
    "postcss-loader": "0.8.2",
    "react": "15.0.1",
    "react-addons-css-transition-group": "15.0.1",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "15.0.1",
    "sass-loader": "3.2.0",
    "scrollbar-width": "3.1.1",
    "simply-uuid": "1.0.1",
    "sniffr": "1.1.4",
    "style-loader": "0.13.0",
    "svg-sprite-loader": "0.0.18",
    "url-loader": "0.5.5",
    "whatwg-fetch": "0.7.0"
  }
}
