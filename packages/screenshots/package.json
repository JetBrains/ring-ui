{
  "name": "@ring-ui/screenshots",
  "version": "6.0.0",
  "description": "Screenshot tests runner for Ring UI library",
  "author": "JetBrains",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "pregui": "port-claim 8000",
    "gather": "npm run screenshots -- --update-refs",
    "gui": "npm run screenshots gui",
    "screenshots": "node screenshots-local.js",
    "serve": "http-server ../../storybook-dist/ -p 9999",
    "test": "npm run screenshots",
    "test-ci": "node screenshots-ci.js --teamcity",
    "cleanup": "npm run screenshots remove-unused-screens -- -p 'testplane/**/*.png' --skip-questions"
  },
  "devDependencies": {
    "@jetbrains/testplane-teamcity-reporter": "^1.2.2",
    "axios": "^1.13.2",
    "browserstack-local": "^1.5.8",
    "dotenv": "^17.2.3",
    "filenamify": "^7.0.1",
    "font-awesome": "^4.7.0",
    "fs-extra": "^11.3.2",
    "html-reporter": "^11.4.0",
    "http-server": "^14.1.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "port-claim": "^3.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "testplane": "^8.34.4",
    "yauzl": "^3.2.0"
  }
}
