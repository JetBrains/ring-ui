{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "private": true,
  "config": {
    "components": "./src/components",
    "dist": "./dist"
  },
  "scripts": {
    "create-component": "yo ring-ui:react --path $npm_package_config_components",
    "build": "webpack -p --devtool sourcemap",
    "test": "karma start",
    "lint": "eslint .",
    "ci-test": "npm run lint && karma start --teamcity",
    "start": "webpack-dev-server -d --history-api-fallback --inline --hot --colors --port <%= port %>"
  },
  "babel": {
    "presets": "jetbrains"
  },
  "dependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.16.0",
    "babel-preset-jetbrains": "1.0.6",
    "css-loader": "0.25.0",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "jetbrains-icons": "<%= jetbrainsIcons %>",
    "jetbrains-logos": "<%= jetbrainsLogos %>",
    "postcss-cssnext": "2.8.0",
    "postcss-loader": "1.2.2",
    "postcss-modules-values-replace": "1.2.1",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "redbox-react": "1.3.1",
    "ring-ui": "<%= ringUi %>",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "electron-prebuilt": "1.4.3",
    "eslint": "3.7.1",
    "eslint-config-jetbrains": "2.0.19",
    "generator-ring-ui": "<%= generatorRingUi %>",
    "karma": "1.3.0",
    "karma-chai-plugins": "0.8.0",
    "karma-electron-launcher": "0.1.0",
    "karma-mocha": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "1.0.0",
    "karma-webpack": "1.8.0",
    "mocha": "3.1.2",
    "yo": "1.8.5"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
