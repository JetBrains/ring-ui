    "fs-extra": "^9.0.1",
    "hermione": "^3.9.0",
    "html-reporter": "^5.16.4",
    "http-server": "^0.12.3",
    "ip": "^1.1.5",
    "jest": "^26.4.2",
    "kill-port": "^1.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
