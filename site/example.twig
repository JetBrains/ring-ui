<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>{{ example.name }}</title>
  <script src="{{ publicPath }}dll.js"></script>
  <script src="{{ publicPath }}example-common.js"></script>
</head>
<body>

<!-- example content -->
{% for file in example.files if file.type == 'html' %}
  {% for asset in file.assets if asset.type == 'html' %}
    {{ asset.content | raw }}
  {% endfor %}
{% endfor %}

<!-- example assets -->
{% for file in example.files %}
  {% for asset in file.assets %}
    {% if asset.type == 'js' %}
      <script src="{{ publicPath }}{{ asset.path }}"></script>
    {% endif %}
  {% endfor %}
{% endfor %}

</body>
</html>
